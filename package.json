{
  "name": "pairsniper",
  "version": "1.0.0",
  "description": "A comprehensive API for tracking cryptocurrency pairs, providing real-time data and insights.",
  "main": "app.js",
  "scripts": {
    "setup-dbs": "psql -f db/setup.sql",
    "seed": "node ./db/seeds/run-seed.js",
    "seed-prod": "cross-env NODE_ENV=production npm run seed",
    "start": "node app.js",
    "prepare": "husky install",
    "test": "jest --coverage",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaizen-development/pairsniper.git"
  },
  "keywords": [
    "crypto",
    "tracking",
    "api",
    "real-time",
    "cryptocurrency",
    "pairs"
  ],
  "author": "Mohammed Saabir Ahmed (https://github.com/ProdiGeeZ), Ima Khan (https://github.com/imaa04), Kyle Powis (https://github.com/kylexpowis)",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.8",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.1",
    "pg": "^8.11.3",
    "pg-format": "^1.0.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "husky": "^8.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
